            });
        //Expand card containing the current carousel item and highlight toggle button corresponding to the current carousel item
        A.on("a:carousel:prime-main-carousel:change:pageNumber", function(data) {
            A.$(".card-content .carousel-page-button").removeClass("carousel-button-selected").parents(".card-category").removeClass("card-selected");
